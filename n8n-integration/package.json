{
  "name": "@maple/n8n-nodes-maple",
  "version": "1.0.0",
  "description": "MAPLE (Multi Agent Protocol Language Engine) nodes for n8n - Visual Multi-Agent AI Workflows",
  "keywords": [
    "n8n-community-node-package",
    "maple",
    "agent",
    "ai",
    "multi-agent",
    "protocol",
    "communication",
    "mahesh-vaikri",
    "visual-workflows",
    "automation"
  ],
  "license": "AGPL-3.0-only",
  "homepage": "https://github.com/maheshvaikri-code/maple-oss",
  "author": {
    "name": "Mahesh Vaijainthymala Krishnamoorthy (Mahesh Vaikri)",
    "email": "mahesh@maple.dev",
    "url": "https://github.com/maheshvaikri-code"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/maheshvaikri-code/maple-oss.git"
  },
  "bugs": {
    "url": "https://github.com/maheshvaikri-code/maple-oss/issues"
  },
  "main": "index.js",
  "scripts": {
    "build": "tsc && gulp build:icons",
    "dev": "tsc --watch",
    "format": "prettier nodes credentials --write",
    "lint": "eslint nodes credentials package.json",
    "demo": "node demo/run-demo.js",
    "demo:quick": "node demo/quick-demo.js",
    "demo:full": "node demo/full-demo.js",
    "test": "jest",
    "test:integration": "jest --testPathPattern=integration",
    "test:performance": "jest --testPathPattern=performance",
    "test:load": "node tests/load-test.js",
    "test:stress": "node tests/stress-test.js",
    "start:broker": "node demo/start-maple-broker.js",
    "launch": "node launch.js",
    "launch:dev": "NODE_ENV=development node launch.js",
    "launch:production": "NODE_ENV=production node launch-production.js",
    "launch:all": "npm run start:broker && npm run demo:full",
    "deploy": "npm run launch:production",
    "stop": "pm2 delete maple-server || pkill -f 'node launch'",
    "restart": "npm run stop && npm run launch:production",
    "logs": "pm2 logs maple-server || tail -f logs/maple-prod.log",
    "metrics": "curl http://localhost:9090/metrics",
    "health": "curl http://localhost:8080/health",
    "status": "pm2 status || ps aux | grep maple",
    "cleanup": "npm run stop && rm -f logs/*.log deployment-*.json",
    "install:global": "npm install -g @maple/n8n-nodes-maple",
    "publish:npm": "npm run build && npm run test && npm publish",
    "publish:github": "npm run build && gh release create v$npm_package_version",
    "validate": "npm run lint && npm run test && npm run build",
    "prepublishOnly": "npm run validate"
  },
  "files": [
    "dist",
    "assets",
    "workflows",
    "config",
    "docs",
    "README.md",
    "CHANGELOG.md",
    "LICENSE",
    "launch.js",
    "launch-production.js"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/MAPLEApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/MAPLEAgent/MAPLEAgent.node.js",
      "dist/nodes/MAPLECoordinator/MAPLECoordinator.node.js",
      "dist/nodes/MAPLEResourceManager/MAPLEResourceManager.node.js"
    ]
  },
  "devDependencies": {
    "@types/node": "^18.16.16",
    "@types/jest": "^29.5.2",
    "@types/ws": "^8.5.4",
    "@types/uuid": "^9.0.2",
    "@types/express": "^4.17.17",
    "@types/cors": "^2.8.13",
    "n8n-workflow": "^1.0.0",
    "typescript": "^5.1.6",
    "ts-jest": "^29.1.0",
    "jest": "^29.5.0",
    "prettier": "^2.8.8",
    "eslint": "^8.42.0",
    "@typescript-eslint/eslint-plugin": "^5.59.11",
    "@typescript-eslint/parser": "^5.59.11",
    "gulp": "^4.0.2",
    "gulp-typescript": "^6.0.0-alpha.1",
    "loadtest": "^7.0.0"
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  },
  "dependencies": {
    "axios": "^1.4.0",
    "ws": "^8.13.0",
    "uuid": "^9.0.0",
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "jsonwebtoken": "^9.0.0",
    "dotenv": "^16.1.4",
    "winston": "^3.9.0",
    "prom-client": "^14.2.0"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "os": [
    "linux",
    "darwin",
    "win32"
  ],
  "cpu": [
    "x64",
    "arm64"
  ],
  "funding": {
    "type": "github",
    "url": "https://github.com/maheshvaikri-code"
  },
  "config": {
    "creator": "Mahesh Vaijainthymala Krishnamoorthy (Mahesh Vaikri)",
    "attribution": "Creator: Mahesh Vaijainthymala Krishnamoorthy (Mahesh Vaikri)",
    "project": "MAPLE - Multi Agent Protocol Language Engine"
  }
}